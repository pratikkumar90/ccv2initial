 {
    "commerceSuiteVersion": "6.7.0.1",
    "extensions": [
        "kiwi",
        "tiger",
        "privacyoverlayeraddon",
        "yacceleratorstorefront",
        "backoffice"
    ],
    "properties": [
        {
            "key": "test.property.1",
            "value": "test.property.1.value",
        },
        {
            "key": "test.property.2",
            "value": "test.property.2.value",
            "persona": "development"
        },
        {
            "key": "test.property.2",
            "value": "test.property.2.value.in.prod.only",
            "persona": "production"
        }
    ],
    "aspects": [
        {
            "name": "accstorefront",
            "properties": [
                {
                    "key": "spring.session.enabled",
                    "value": "true"
                },
                {
                    "key": "spring.session.albinostorefront.save",
                    "value":"async"
                },
                {
                    "key": "spring.session.albinostorefront.cookie.name",
                    "value": "JSESSIONID"
                },
                {
                    "key": "spring.session.albinostorefront.cookie.path",
                    "value": "/"
                },
                {
                    "key":"storefrontContextRoot",
                    "value":""
                }
            ],
            "webapps": [
                {
                    "name": "mediaweb",
                    "contextPath": "/medias"
                },
                {
                    "name": "albinostorefront",
                    "contextPath": ""
                },
                {
                    "name": "acceleratorservices",
                    "contextPath": "/acceleratorservices"
                }
            ]
        }
    ],
    "tests": {
        "extensions": ["kiwi", "tiger"],
        "annotations": ["UnitTests", "IntegrationTests"],
        "packages": ["de.hybris.infra.*"]
    },
    "webTests": {
        "extensions": ["yacceleratorstorefront"],
        "excludedPackages": ["de.hybris.platform.*"]
    }
}
